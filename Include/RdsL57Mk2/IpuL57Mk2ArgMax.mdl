  //@L57CMD_IF_END_1
  //@L57CMD_IF_BEGIN_1 /*RDSTYPE0*/ EQ "D"
  /*VAR0*/_trans=_Transfer.AddParameterRds("/*VAR0*/",L57_VARSUFFIX_NONE,FALSE,/*VAR0*/.Num(),&/*VAR0*/);
