   begin
    rect from 0 0 size 346 105 solid color 0
    picture pos 0 0 xvar "x" yvar "y"
     begin
      line from 0 -61 to 0 59 solid width 5 color 0
      line from -49 0 to 63 0 solid width 4 color 0
     end
   end
