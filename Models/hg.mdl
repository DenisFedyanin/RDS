$BC55MODEL 0.0.0 2017.4.12.12.10.1 0
Модель блока для модуля автокомпиляции RDS BCpp55@Common.dll
$VARS
  signal name "Start" in menu run default 0
  signal name "Ready" out menu default 0
  double name "X" in menu default "0"
  double name "Y" out menu default "0"
  matrix double name "Z" out menu default "[ ] 0"
$ENDDECL
$MODEL
Y=X*0.3;

Z.Resize(4,5);

if (Z.IsEmpty()) return;

int c=Z.Cols();
int r=Z.Rows();

for (int i=0; i<r; i++) 
	for (int j=0; j<c; j++) 
		Z[i][j]=i+j;



$OPTIONS
  tabs $MODEL
  window pos -3829 177 size 640 470 hdiv 337 vdiv 170 fdiv 160
  svc 0 90 1 90 2 90 3 90
  dvc 0 90 1 90 2 90
  fnc 0 90 1 90 2 90
  pvc 0 90 1 90 2 100
  dvcheck 1
  indexcheck 1
  initcalc 0
  initcalcstart 0
  ignorewarnings 0
  customerrmsg 0
  autoreadonlyeditor 0
  genvartypemacros 0
  matherr 1
  catchall 0
  dvstart 1
  searchlinks auto deep
