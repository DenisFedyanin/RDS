$BC55MODEL 0.0.0 2016.10.6.15.2.0 0
Модель блока для модуля автокомпиляции RDS BCpp55@Common.dll
$VARS
  signal name "Start" in menu run default 0
  signal name "Ready" out menu default 0
  string name "fileName" in menu run default ""
  signal name "fileExist" out menu default 0
$ENDDECL
$MODEL
if ( access(fileName.c_str(),F_OK) == 0 ) {
	Start = 0;
	fileExist = 1;
	Ready = 1;
} else {
	Start = 1;
	fileExist = 0;
	Ready = 0;
}
$GLOBAL
#include <unistd.h>
$OPTIONS
  tabs $MODEL $GLOBAL
  window pos 120 271 size 903 470 hdiv 337 vdiv 171 fdiv 161
  svc 0 90 1 90 2 90 3 90
  dvc 0 90 1 90 2 90
  fnc 0 90 1 90 2 90
  pvc 0 90 1 90 2 100
  dvcheck 1
  indexcheck 1
  ignorewarnings 0
  customerrmsg 0
  autoreadonlyeditor 0
  genvartypemacros 0
  matherr 1
  catchall 0
  dvstart 1
  searchlinks auto deep
