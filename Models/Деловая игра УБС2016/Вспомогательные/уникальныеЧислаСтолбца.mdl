$BC55MODEL 0.0.0 2016.10.12.21.48.1 0
Модель блока для модуля автокомпиляции RDS BCpp55@Common.dll
$VARS
  signal name "Start" in menu run default 0
  signal name "Ready" out menu default 0
  matrix int name "M" in menu run default "[ ] 0"
  int name "count" out menu default 0
$ENDDECL
$MODEL
// работает если повторяющиеся числа идут подряд (как при сортировке)
count = M.Rows();
if ( count == 0 ) return;
int d = M[0][0];
for ( int i = 1 ; i < M.Rows() ; i++ ) {
  if( abs(d - M[i][0]) < 0.01 ) {
    count--;
  } else {
    d = M[i][0];
  }
}
$OPTIONS
  tabs $MODEL
  window pos 104 108 size 880 526 hdiv 337 vdiv 198 fdiv 188
  svc 0 90 1 90 2 90 3 90
  dvc 0 90 1 90 2 90
  fnc 0 90 1 90 2 90
  pvc 0 90 1 90 2 100
  dvcheck 1
  indexcheck 1
  ignorewarnings 0
  customerrmsg 0
  autoreadonlyeditor 0
  genvartypemacros 0
  matherr 1
  catchall 0
  dvstart 1
  searchlinks auto deep
