EXPBLKTXT
version 1

dllblock name "ExCalc"
 begin
  name hidden
  options sizeable
  vars
   begin
    signal name "_Start" in run default 0
    signal name "_Ready" out default 0
    signal name "Start" in menu run default 0
    signal name "Stop" in menu run default 0
   end
  layout rect 90 40 fill 16777215 line 0 text "Отдельный\r\nрасчет" font "Arial" height 13 color 0 width 1
  dll file "$DLL$\\Common.dll" func "ExclusiveCalc" setup "" auto
 end
