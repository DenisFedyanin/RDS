EXPBLKTXT
version 1

dllblock name "SumM1"
 begin
  options sizeable
  vars
   begin
    signal name "Start" in run default 0
    signal name "Ready" out default 0
    matrix double oneindex name "X" in menu run default "[ ] 0"
    double name "y" out menu hide default 0
   end
  layout rect 30 30 fill 16777215 line 0 text "S" font "Symbol" height 20 bold color 0 charset symbol width 1 scalewidth notranslate
  dll file "$DLL$\\Common.dll" func "SumMDouble" setup "" auto
 end
