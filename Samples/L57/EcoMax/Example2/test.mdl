$BC55MODEL 0.0.0 2017.4.19.20.32.2 0
Модель блока для модуля автокомпиляции RDS BCpp55@Common.dll
$VARS
  signal name "Start" in menu run default 0
  signal name "Ready" out menu default 0
  matrix double name "Y" out menu default "[ ] 0"
$ENDDECL
$MODEL
int n= 200;
double x0=-5;
double dx=0.1;


Y.Resize(n,2);

for (int i=0;i<n;i++) {
	Y[i][0]=dx*i;	
	Y[i][1]=sin(dx*i);	
};
$OPTIONS
  tabs $MODEL
  window pos -1648 208 size 1134 669 hdiv 337 vdiv 273 fdiv 257
  svc 0 90 1 90 2 90 3 90
  dvc 0 90 1 90 2 90
  fnc 0 90 1 90 2 90
  pvc 0 90 1 90 2 100
  dvcheck 1
  indexcheck 1
  initcalc 0
  initcalcstart 0
  ignorewarnings 0
  customerrmsg 0
  autoreadonlyeditor 0
  genvartypemacros 0
  matherr 1
  catchall 0
  dvstart 1
  searchlinks auto deep
