// Занесение стационарных переменных в вычислитель функции
void rdsbcppBlockClass::L57InputsToCalc(void)
{
  _Calculator.A=A;
  _Calculator.w=w;
  _Calculator.k=k;
  _Calculator.l57_01000000_min=l57_01000000_min;
  _Calculator.l57_01000000_max=l57_01000000_max;
}
