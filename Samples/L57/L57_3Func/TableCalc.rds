global
 begin
  object name "L57_3Func.mdl" type "ICS.l49.Common.dll.bcpp55.model.scheme.$BC55MODEL" id 0 string "$BC55MODEL\r\nАвтоматически генерируемая модель вычислителя табличных фу"+
