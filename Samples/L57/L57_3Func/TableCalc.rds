  +"-------------------\r\n\r\n// Проверка изменения переменных\r\n// *pChgSigna"+
  +"lOn взводится при наличии сигнала изменения (независимо от фактическог"+
  +"о изменения)\r\nBOOL rdsbcppBlockClass::L57InputsChange(BOOL _force,BOOL"+
  +" *pChgSignalOn)\r\n{ BOOL _changes=FALSE;\r\n\r\n  // Стационарных веществен"+
  +"ных входов: 1\r\n  // Служебных вещественных входов: 4\r\n  // Итого вещес"+
  +"твенных: 5\r\n\r\n  // Вещественные входы\r\n  if(_allinval_dchg!=0 || _forc"+
  +"e)\r\n    { _allinval_dchg=0;\r\n      _changes=_force;\r\n      if(pChgSign"+
  +"alOn) *pChgSignalOn=TRUE;\r\n\r\n      if(k_old!=k)\r\n        { _changes=TR"+
  +"UE;\r\n          _Transfer.TransferParameter(rdsbcppBlockData->Block,k_t"+
  +"rans,_Transfer.DestBlocks,TRUE);\r\n        }\r\n\r\n      if(x_min_old!=x_m"+
