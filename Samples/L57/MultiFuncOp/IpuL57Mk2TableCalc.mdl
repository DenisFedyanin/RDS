// Инициализация фиксированных значений
void rdsbcppBlockClass::L57_Init_Fixed_Data(void)
{
    x_0=0;
    x_step=0.1;
    y_0=0;
    y_step=0.1;

}
//-----------------------------------------------------------------------

// Создание всех параметров передачи
void rdsbcppBlockClass::L57_Create_Trans_Data(void)
{
  // Стационарных входов: 1
  // Максимизируемых параметров: 2
  _Transfer.ClearParameters();
  _Transfer.EnlargeParameters(1+2*3);

  A_trans=_Transfer.AddParameterRds("A",L57_VARSUFFIX_NONE,FALSE,A.Num(),&A);

  x_min_trans=_Transfer.AddParameterRds("x",L57_VARSUFFIX_MIN,FALSE,x_min.Num(),&x_min);
  x_max_trans=_Transfer.AddParameterRds("x",L57_VARSUFFIX_MAX,FALSE,x_max.Num(),&x_max);
  _Transfer.AddParameter("x",L57_VARSUFFIX_STEP,FALSE,-1,&x_step);

  y_min_trans=_Transfer.AddParameterRds("y",L57_VARSUFFIX_MIN,FALSE,y_min.Num(),&y_min);
  y_max_trans=_Transfer.AddParameterRds("y",L57_VARSUFFIX_MAX,FALSE,y_max.Num(),&y_max);
  _Transfer.AddParameter("y",L57_VARSUFFIX_STEP,FALSE,-1,&y_step);

}
//-----------------------------------------------------------------------

// Проверка изменения переменных
// *pChgSignalOn взводится при наличии сигнала изменения (независимо от фактического изменения)
BOOL rdsbcppBlockClass::L57InputsChange(BOOL _force,BOOL *pChgSignalOn)
{ BOOL _changes=FALSE;

  // Стационарных вещественных входов: 1
  // Служебных вещественных входов: 4
  // Итого вещественных: 5

  // Вещественные входы
  if(_allinval_dchg!=0 || _force)
    { _allinval_dchg=0;
      _changes=_force;
      if(pChgSignalOn) *pChgSignalOn=TRUE;

      if(A_old!=A)
        { _changes=TRUE;
          _Transfer.TransferParameter(rdsbcppBlockData->Block,A_trans,_Transfer.DestBlocks,TRUE);
        }

      if(x_min_old!=x_min)
        { _changes=TRUE;
          _Transfer.TransferParameter(rdsbcppBlockData->Block,x_min_trans,_Transfer.DestBlocks,TRUE);
        }
      if(x_max_old!=x_max)
        { _changes=TRUE;
          _Transfer.TransferParameter(rdsbcppBlockData->Block,x_max_trans,_Transfer.DestBlocks,TRUE);
        }
      if(y_min_old!=y_min)
        { _changes=TRUE;
          _Transfer.TransferParameter(rdsbcppBlockData->Block,y_min_trans,_Transfer.DestBlocks,TRUE);
        }
      if(y_max_old!=y_max)
        { _changes=TRUE;
          _Transfer.TransferParameter(rdsbcppBlockData->Block,y_max_trans,_Transfer.DestBlocks,TRUE);
        }

      if(_changes)
        { // Запоминание изменившихся
          A_old=A;
          x_min_old=x_min;
          x_max_old=x_max;
          y_min_old=y_min;
          y_max_old=y_max;
        }
    }

  // Матричные входы (матрицы, массивы, функции)

  return _changes;
}
//-----------------------------------------------------------------------

// Занесение стационарных переменных в вычислитель функции
void rdsbcppBlockClass::L57InputsToCalc(void)
{
  _Calculator.A=A;
}
//-----------------------------------------------------------------------

// Инициализация набора перебираемых параметров
void rdsbcppBlockClass::L57InitParams(void)
{
  _Params.P[0].Min=x_min;
  _Params.P[0].Max=x_max;
  _Params.P[0].Step=x_step; // 0.1

  _Params.P[1].Min=y_min;
  _Params.P[1].Max=y_max;
  _Params.P[1].Step=y_step; // 0.1

}
//-----------------------------------------------------------------------

// Занесение текущих значений параметров в вычислитель функции
void rdsbcppBlockClass::L57ParamsToCalc(void)
{
  _Calculator.x=_Params.P[0].Current;
  _Calculator.y=_Params.P[1].Current;
}
