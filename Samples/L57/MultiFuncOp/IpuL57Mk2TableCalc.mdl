// Служебный класс для вычисления значения функции
#define ERRORVALUE (L57_ERROR())
class L57_Calc
{
  public:
    inline double L57_ERROR(void){return rdsbcppHugeDouble;};
    double x;
    double y;
    double A;
    // Внутренние функции (генерируемые)
    double _f_f1(double x);
    double _f_f2(double A,double x);
    // Функция вычисления (генерируемая)
    double _Calc_gen(void);
    // Функция вычисления (с перехватом исключений)
    double _Calc_main(void)
    { double __temp;
      RDSBCPP_TRY
        { __temp=_Calc_gen(); }
      RDSBCPP_CATCHALL
        { __temp=rdsbcppHugeDouble; }
      return __temp;
    };
};
// Внутренние функции (генерируемые)
double L57_Calc::_f_f1(double x)
{
return sin(x); // 8
; /* */
  return rdsbcppHugeDouble;
}
double L57_Calc::_f_f2(double A,double x)
{
return sin(A*x); // 4
; /* */
  return rdsbcppHugeDouble;
}
// Функция вычисления (генерируемая)
double L57_Calc::_Calc_gen(void)
{
return _f_f1(x)+_f_f2(A,y);
; /* */
  return rdsbcppHugeDouble;
}
#undef ERRORVALUE
